{
  "name": "chat-service",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node dist/main.js\"",
    "start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/main.js\"",
    "start:prod": "node dist/main.js",
    "start:test": "cross-env NODE_ENV=test nodemon",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "lint:fix": "tslint --fix -p tsconfig.json -c tslint.json",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json --detectOpenHandles",
    "rand": "node -e \"console.log(require('crypto').randomBytes(256).toString('base64'));\""
  },
  "dependencies": {
    "@nest-modules/mailer": "^1.1.3",
    "@nestjs/common": "^6.11.9",
    "@nestjs/core": "^6.11.9",
    "@nestjs/cqrs": "^6.1.0",
    "@nestjs/jwt": "^6.1.2",
    "@nestjs/mongoose": "^6.3.1",
    "@nestjs/passport": "^6.2.0",
    "@nestjs/platform-express": "^6.11.9",
    "@nestjs/platform-socket.io": "^6.11.9",
    "@nestjs/platform-ws": "^6.11.9",
    "@nestjs/websockets": "^6.11.9",
    "bcrypt": "^4.0.1",
    "cache-manager": "^3.1.0",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "compression": "^1.7.4",
    "cookie": "^0.4.0",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "express-rate-limit": "^5.1.1",
    "express-slow-down": "^1.3.1",
    "helmet": "^3.21.3",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "mongoose": "^5.9.3",
    "mongoose-paginate-v2": "^1.3.52",
    "mongoose-unique-validator": "^2.0.3",
    "nest-event": "^1.0.8",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pug": "^2.0.4",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "socket.io-emitter": "^3.1.1",
    "socket.io-redis": "^5.2.0",
    "uuid": "^7.0.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.11.9",
    "@types/bcrypt": "^3.0.0",
    "@types/compression": "^1.7.0",
    "@types/cookie": "^0.3.3",
    "@types/dotenv": "^8.2.0",
    "@types/express": "4.17.2",
    "@types/express-rate-limit": "^5.0.0",
    "@types/express-slow-down": "^1.3.0",
    "@types/helmet": "0.0.45",
    "@types/jest": "25.1.3",
    "@types/lodash": "^4.14.149",
    "@types/mongoose": "^5.7.3",
    "@types/mongoose-paginate-v2": "^1.3.1",
    "@types/mongoose-unique-validator": "^1.0.3",
    "@types/node": "13.7.7",
    "@types/passport-google-oauth20": "^2.0.3",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/pug": "^2.0.4",
    "@types/redis": "^2.8.16",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-redis": "^1.0.25",
    "@types/supertest": "2.0.8",
    "@types/uuid": "^7.0.0",
    "@types/ws": "^7.2.2",
    "cross-env": "^7.0.0",
    "jest": "25.1.0",
    "nodemon": "^2.0.2",
    "prettier": "1.19.1",
    "supertest": "4.0.2",
    "ts-jest": "25.2.1",
    "ts-node": "8.6.2",
    "tsc-watch": "4.2.3",
    "tsconfig-paths": "3.9.0",
    "tslint": "6.0.0",
    "typescript": "3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
